<body>
<div class="wrap-body">

<!--////////////////////////////////////Header-->
<header>
	<div class="zerogrid">
		<div class="wrap-header">
			<div class="row">
				<div class="col-1-3">
					<div class="wrap-col">
						<div class="logo"><a href="#"><img src="images/logo.png"/></a></div>	
					</div>
				</div>
				<div class="col-1-3 offset-1-3">
					<div class="wrap-col">
						<form method="get" action="/search" id="search" class="f-right">
						  <input name="q" type="text" size="40" placeholder="Search..." />
						</form>
					</div>
				</div>
			</div>
		</div>
		<nav id="menu-wrap"><div id="menu-trigger">Menu</div>    
			<ul id="menu" style="display: none;">
				<li><a href="/">Accueil</a></li>
				<li>
					<a href="article">Article</a>
					<ul>
						<li>
							<a href="coucher">Chambre Ã  Coucher</a>
						</li>
                        <li>
							<a href="cuisine">Cuisine</a>
						</li>
                        <li>
							<a href="salon">Salon</a>
						</li>
                        <li>
							<a href="salle">Salle</a>
						</li>
						
				
			</ul>
					
			
			<li><a href="espace">Espace Client</a></li>
            
		</nav>
	</div>
</header>

<section id="container">
	<div class="wrap-container">
		<div class="zerogrid">
			<div class="row">
				
				<div class="col-1-3">
					
				</div>
				<div class="col-2-3">
						<div class="wrap-col">
							<div class="contact">
								<h3>Connexion</h3>
								<div id="contact_form">
									<form name="form1" id="ff" method="post" action="{{ path('security_login_check') }}">
									{% if error %}
                            <div class="alert alert-danger">
                                {{ error.messageKey|trans(error.messageData) }}
                            </div>
                        {% endif %}
										<label class="row">
											<div class="col-1-2">
												<div class="wrap-col">
													<input type="text"  name="_email" value="{{ last_username }}" id="name" placeholder="login" required="required" />
												</div>
											</div>
											
										</label>
										<label class="row">
											
											<div class="col-1-2">
												<div class="wrap-col">
													<input type="password" name="email" id="email" placeholder="password" required="required" name="_password"/>
												</div>
											</div>
										</label>
										
										<input class="sendButton" type="submit" name="Submit" value="Connexion">
									    {% if app.user == null %}
                                        <a href="accunt">CREER COMPTE?</a>
										{% endif %}
									</form>
								</div>
							</div>
						</div>
					</div>
			</div>
		</div>
	</div>
</section>

<script type="text/javascript">
    $(function() {
		if ($.browser.msie && $.browser.version.substr(0,1)<7)
		{
		$('li').has('ul').mouseover(function(){
			$(this).children('ul').css('visibility','visible');
			}).mouseout(function(){
			$(this).children('ul').css('visibility','hidden');
			})
		}

		/* Mobile */
		$("#menu-trigger").on("click", function(){
			$("#menu").slideToggle();
		});

		// iPad
		var isiPad = navigator.userAgent.match(/iPad/i) != null;
		if (isiPad) $('#menu ul').addClass('no-transition');      
    });          
</script>

</div>
</body></html>